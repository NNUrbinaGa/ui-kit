{
  "namedInputs": {
    "production": [
      "sharedGlobals",
      "sharedTypescriptSources",
      "{projectRoot}/stencil.config.ts",
      "{projectRoot}/collection-manifest.json",
      "{projectRoot}/tailwind.config.js",
      "{projectRoot}/svg.transform.js",
      "{projectRoot}/stencil-plugin/**",
      "!{projectRoot}/src/components.d.ts",
      "!{projectRoot}/src/external-builds/**",
      "!{projectRoot}/src/generated/**",
      "!{projectRoot}/src/components/search/atomic-search-interface/lang/**"
    ]
  },
  "targets": {
    "cached:build:stencil": {
      "dependsOn": ["^build", "build:locales"],
      "inputs": ["^production", "production"],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/www",
        "{projectRoot}/docs",
        "{projectRoot}/loader",
        "{projectRoot}/src/components.d.ts"
      ],
      "executor": "nx:run-commands",
      "options": {
        "command": "node --max_old_space_size=6144 ../../node_modules/@stencil/core/bin/stencil build",
        "cwd": "packages/atomic"
      }
    },
    "validate:definitions": {
      "dependsOn": ["^build", "cached:build:stencil"],
      "inputs": [],
      "outputs": [],
      "executor": "nx:run-script",
      "options": {
        "script": "validate:definitions"
      }
    },
    "validate:storybook": {
      "dependsOn": ["^build"],
      "inputs": [],
      "outputs": [],
      "executor": "nx:run-commands",
      "options": {
        "command": "node node_modules/.bin/lit-analyzer ../../packages/atomic/dist/types/components.d.ts ../../packages/atomic/src/components/**/*.stories.tsx",
        "cwd": "utils/atomic-storybook"
      }
    },
    "build:storybook:assets-list": {
      "dependsOn": ["cached:build:stencil"],
      "inputs": [],
      "outputs": [],
      "executor": "nx:run-commands",
      "options": {
        "command": "node scripts/list-assets.mjs",
        "cwd": "packages/atomic"
      }
    },
    "build:storybook": {
      "dependsOn": [
        "cached:build:stencil",
        "validate:storybook",
        "build:storybook:assets-list"
      ],
      "inputs": [],
      "outputs": [],
      "executor": "nx:run-commands",
      "options": {
        "command": "node node_modules/.bin/build-storybook --config-dir ./.storybook --static-dir ../../packages/atomic/dist --output-dir ../../packages/atomic/storybook-static",
        "cwd": "utils/atomic-storybook"
      }
    },
    "dev:storybook": {
      "dependsOn": ["cached:build:stencil", "validate:storybook"],
      "inputs": [],
      "outputs": [],
      "executor": "nx:run-commands",
      "options": {
        "command": "node node_modules/.bin/start-storybook --config-dir ./.storybook --static-dir ../../packages/atomic/dist -p 6006",
        "cwd": "utils/atomic-storybook"
      }
    },
    "build": {
      "dependsOn": [
        "cached:build:stencil",
        "build:storybook",
        "validate:storybook",
        "validate:definitions"
      ],
      "inputs": [],
      "outputs": [],
      "executor": "nx:noop"
    }
  }
}
